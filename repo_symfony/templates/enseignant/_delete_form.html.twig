<form method="post" action="{{ path('app_enseignant_delete', {'id': enseignant.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ enseignant.id) }}">
    
    <!-- Bouton de supprimer-->
    <button type="submit" class="btn btn-danger btn-sm">
        <i class="bi bi-trash"></i> Supprimer
    </button>
</form>
